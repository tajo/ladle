    "test": "yarn test-prod && yarn test-dev"
  },
  "dependencies": {
    "@ladle/react": "^0.13.0",
    "@playwright/test": "1.19.2",
    "http-server": "^14.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "start-server-and-test": "^1.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
