{
  "name": "test-commonjs",
  "version": "0.2.34",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "pnpm --filter @ladle/react build && node ../../packages/ladle/cjs/lib/cli/cli.js serve -p 61101",
    "serve-prod": "pnpm --filter @ladle/react build && node ../../packages/ladle/cjs/lib/cli/cli.js build && node ../../packages/ladle/cjs/lib/cli/cli.js preview -p 61101",
    "lint": "echo 'no lint'",
    "test-dev": "start-server-and-test serve 61101 'npx playwright test'",
    "test-prod": "start-server-and-test serve-prod 61101 'npx playwright test'",
    "test": "npm run test-prod && npm run test-dev"
  },
  "dependencies": {
    "@ladle/react": "workspace:*",
    "@playwright/test": "1.23.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^1.14.0"
  }
}
