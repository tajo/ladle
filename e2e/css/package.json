    "test": "npm run test-dev && npm run test-prod"
  },
  "dependencies": {
    "@ladle/react": "workspace:*",
    "@playwright/test": "1.19.2",
    "autoprefixer": "^10.4.5",
    "http-server": "^14.1.0",
    "postcss": "^8.4.12",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23"
  }
}
