{
  "name": "test-css",
  "version": "0.0.20",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "ladle serve --port=61102",
    "serve-prod": "http-server build -c-1 -s -p 61102",
    "build": "ladle build",
    "lint": "echo 'no lint'",
